<h1>All Projects</h1>
<ul>
<% @projects.each do |project| %>
    <%= link_to project_path(project) do %>
      <li>
        <h3><%= project.title %></h3>
        <p><%= project.description %></p>
      </li>
    <% end %>
<% end %>
</ul>

<%= link_to "New Project", new_project_path %>
<%= link_to "All Polls without project", polls_path %>
